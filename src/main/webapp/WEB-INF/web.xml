
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>MyStudentApp</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	<context-param>
	<param-name>author</param-name>
	<param-value>Avakash</param-value>
	</context-param>
	
	<context-param>
	<param-name>dname</param-name>
	<param-value>com.mysql.cj.jdbc.Driver</param-value>
	</context-param>
	
	<context-param>
	<param-name>user</param-name>
	<param-value>root</param-value>
	</context-param>
	
	<context-param>
	<param-name>password</param-name>
	<param-value>devilxworkshop</param-value>
	</context-param>
	<context-param>
	<param-name>dburl</param-name>
	<param-value>jdbc:mysql://localhost:3306/jdbcservletdb</param-value>
	</context-param>
	

	<servlet>
	<servlet-name>CreateProfile</servlet-name>
	<servlet-class>com.avak.controller.CreateProfileServlet</servlet-class>
	</servlet>
	<servlet>
	<servlet-name>ChangePassword</servlet-name>
	<servlet-class>com.avak.controller.ChangePassServlet</servlet-class>
	</servlet>
	<servlet>
	<servlet-name>loginServlet</servlet-name>
	<servlet-class>com.avak.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AllStudentStudent</servlet-name>
		<servlet-class>com.avak.controller.AllStudentStudent</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>Gsearch</display-name>
		<servlet-name>Gsearch</servlet-name>
		<servlet-class>com.avak.controller.Gsearch</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>Asearch</display-name>
		<servlet-name>Asearch</servlet-name>
		<servlet-class>com.avak.controller.Asearch</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>StudentSearch</display-name>
		<servlet-name>StudentSearch</servlet-name>
		<servlet-class>com.avak.controller.StudentSearch</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>HomeServlet</display-name>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>com.avak.controller.HomeServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>LogoutServlet</display-name>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.avak.controller.LogoutServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>AllStudentStudent</servlet-name>
		<url-pattern>/allstudent</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	<servlet-name>loginServlet</servlet-name>
	<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CreateProfile</servlet-name>
		<url-pattern>/createprofile</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ChangePassword</servlet-name>
		<url-pattern>/changepass</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Gsearch</servlet-name>
		<url-pattern>/gsearch</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Asearch</servlet-name>
		<url-pattern>/asearch</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StudentSearch</servlet-name>
		<url-pattern>/ssearch</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/HomeServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	
</web-app>
